Public Class Calculator
    Private Sub Button1_Click(sender As Object, e As EventArgs) Handles Button1.Click
        If TextBox1.Text <> "0" Then
            TextBox1.Text += "1"
        ElseIf
                TextBox1.Text = "1"
        End If

    End Sub

